# Job bcl2fastq MELISSE
jobify -e test_inti2.e -c 8 -p normal --chrono -t 240 -b 'bcl2fastq --input-dir /env/ig/atelier/illumina/cns/DEV_ILLUMINA/RUN_MISEQ_SINGLE12/211014_MELISSE_DDL9H.DONE/Data/Intensities/BaseCalls --min-log-level WARNING --barcode-mismatches 1 --sample-sheet /env/ig/atelier/illumina/cns/DEV_ILLUMINA/RUN_MISEQ_SINGLE12/211014_MELISSE_DDL9H.DONE/SampleSheet_12.csv --runfolder-dir /env/ig/atelier/illumina/cns/DEV_ILLUMINA/RUN_MISEQ_SINGLE12/211014_MELISSE_DDL9H.DONE --output-dir /env/ig/atelier/illumina/cns/DEV_ILLUMINA/RUN_MISEQ_SINGLE12/211014_MELISSE_DDL9H.DONE/test_inti2 -r 6 -p 8 -w 6 --use-bases-mask Y*,I12,Y*' &


